  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- IdentVersionType +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <xs:simpleType name="IdentVersionT">
    <xs:annotation>
      <xs:documentation>Version aa.b(.c(.d)) (without an introducing "V")</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
